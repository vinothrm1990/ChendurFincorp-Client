<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.app.chendurfincorp.client">

    <application
        android:name=".helper.ChendurFincorp"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".activity.SplashActivity"
            android:theme="@style/Trasnsstatusbartheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".activity.LoginActivity"
            android:theme="@style/Trasnsstatusbartheme" />
        <activity
            android:name=".activity.HomeActivity"
            android:label="@string/title_activity_home"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".activity.Network1Activity"
            android:parentActivityName=".activity.HomeActivity"/>
        <activity android:name=".activity.Network2Activity"
            android:parentActivityName=".activity.HomeActivity"/>
        <activity android:name=".activity.Network3Activity"
            android:parentActivityName=".activity.HomeActivity"/>
    </application>

</manifest>